import io
import os

samples = "./samples"
sample_arr = []
execute = "./a.out"
for filename in os.listdir(samples):
	sample_arr.append(filename)
sample_arr.sort()

for filename in sample_arr:
	os.system("echo ' ' >> myoutput" )
	os.system("echo " + "'" + filename + "'" + '>> myoutput')
	os.system(execute + ' ' + samples + '/' + filename  + ' >> myoutput')
